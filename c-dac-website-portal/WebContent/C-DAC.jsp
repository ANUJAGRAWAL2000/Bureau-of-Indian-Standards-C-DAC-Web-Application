		<form action="details.jsp" method="get">

     

			<fieldset>
				<legend align="center">
					<h1>ENTER USER DETAILS</h1>
				</legend>
		<br>		
				<table width="1000" border="7px" align="center" cellspacing="4px" cellpadding="5px">
					<tr>
						<td><h4>FIRST NAME</h4></td>
						<td><input type="text" name="first_name" id="first_name"
							autocomplete="on" placeholder="First Name"
							title="Enter your first name" value=""
							maxlength="32 data-validators=" required minLength:1 maxLength:32"/></td>
						<td><h4>LAST NAME</h4></td>
						<td><input type="text" name="last_name" id="last_name"
							autocomplete="on" placeholder="Last Name"
							title="Enter your last name" value=""
							maxlength="32 data-validators=" requiredminLength:1maxLength:32"></td>
						<td><h4>EMPLOYEE ID</h4></td>
						<td><input type="text" name="empid" id="empid"
							autocomplete="on" placeholder="Employee Id"
							title="Enter your employee id" value=""
							maxlength="32 data-validators=" requiredminLength:1maxLength:32"></td>
					</tr>

					<tr>
						<td><h4>EMAIL ID</h4></td>
						<td><input type="email" name="email" id="email"
							autocomplete="on" placeholder="Email Id"
							title="Enter your email id" value=""
							maxlength="32 data-validators=" requiredminLength:1maxLength:32"></td>
						<td><h4>PHONE</h4></td>
						<td><input type="text" name="phone" id="phone"
							autocomplete="on" placeholder="Phone"
							title="Enter your phone number" value=""
							maxlength="32 data-validators=" requiredminLength:1maxLength:32"></td>
						<td><h4>ADDRESS</h4></td>
						<td><input type="text" name="address" id="address"
							autocomplete="on" placeholder="Address"
							title="Enter your address" value=""
							maxlength="32 data-validators=" requiredminLength:1maxLength:32"></td>
					</tr>

					<tr>
						<td>
							<div class="clearfix"></div>
							<div class="col-md-5">
								<div class="form-group">
									<h4>DESIGNATION</h4>
								</div>
							</div>
						</td>
						<td>
							<div class="col-md-6">
								<div class="form-group">
									<select onchange="test()" class="form-control" name="id_card"
										id="id_card" autocomplete="on" placeholder="DESIGNATION"
										title="Choose your designation" maxlength="32"
										data-validators="required minLength:1 maxLength:32">
										<option>Select from the options</option>
										<option>Team Member</option>
										<option>Project Lead</option>
								</div>
							</div>
						</td>
						<td>
							<div class="clearfix"></div>
							<div class="col-md-5">
								<div class="form-group">
									<h4>OFFICE BRANCH</h4>
								</div>
							</div>
						</td>
						<td>
							<div class="col-md-6">
								<div class="form-group">
									<select class="form-control" name="branch" id="branch"
										autocomplete="on" placeholder="OFFICE BRANCH"
										title="Choose your office branch"  maxlength="32"
										data-validators="required minLength:1 maxLength:32">
										<option>Select from the options</option>
										<option>Mohali</option>
										<option>Noida</option>
										<option>Delhi</option>
										<option>Kolkata</option>
										<option>Mumbai</option>
										<option>Pune(HQ)</option>
										<option>Hyderabad</option>
										<option>Chennai</option>
										<option>Bengaluru</option>
									</select>
								</div>
							</div>
						</td>
						<td></td>
						<td></td>
					</tr>

					<tr>
						<td></td>
						<td></td>
						<td><input type="submit" value="Join"></td>
						<td><input type="reset" value="Reset"></td>
						<td></td>
						<td></td>
					</tr>

				</table>
				<br>
			</fieldset>
		</form>
